        <member name="T:ChargingPile.API.Controllers.ChargingStationController">
            <summary>
            充电站管理控制器
            </summary>
        </member>
        <member name="M:ChargingPile.API.Controllers.ChargingStationController.CreateStation(ChargingPile.API.Models.DTOs.CreateChargingStationDTO)">
            <summary>
            创建新的充电站
            </summary>
            <param name="dto">充电站创建信息</param>
            <remarks>
            请求示例:
            
                POST /api/ChargingStation
                {
                    "name": "示例充电站",
                    "latitude": 30.123456,
                    "longitude": 120.123456,
                    "address": "示例地址",
                    "totalPorts": 10,
                    "availablePorts": 10
                }
            </remarks>
            <response code="201">充电站创建成功</response>
            <response code="400">输入数据验证失败或该位置已存在充电站</response>
        </member>
        <member name="M:ChargingPile.API.Controllers.ChargingStationController.GetNearbyStations(System.Decimal,System.Decimal,System.Int32)">
            <summary>
            获取指定范围内的附近充电站
            </summary>
            <param name="latitude">纬度</param>
            <param name="longitude">经度</param>
            <param name="radius">搜索半径（米），默认5000米</param>
            <remarks>
            请求示例:
            
                GET /api/ChargingStation/nearby?latitude=30.123456&amp;longitude=120.123456&amp;radius=5000
            </remarks>
            <response code="200">返回符合条件的充电站列表</response>
        </member>
        <member name="M:ChargingPile.API.Controllers.ChargingStationController.GetNearbySpatialStations(System.Decimal,System.Decimal,System.Int32)">
            <summary>
            使用空间索引获取指定范围内的附近充电站
            </summary>
            <param name="latitude">纬度</param>
            <param name="longitude">经度</param>
            <param name="radius">搜索半径（米），默认5000米</param>
            <remarks>
            请求示例:
            
                GET /api/ChargingStation/spatial-nearby?latitude=30.123456&amp;longitude=120.123456&amp;radius=5000
            </remarks>
            <response code="200">返回符合条件的充电站列表</response>
        </member>
        <member name="M:ChargingPile.API.Controllers.ChargingStationController.CalculateDistance(System.Decimal,System.Decimal,System.Decimal,System.Decimal)">
            <summary>
            计算两个经纬度点之间的距离（米）
            </summary>
        </member>
        <member name="M:ChargingPile.API.Controllers.ChargingStationController.GetStation(System.String)">
            <summary>
            获取指定ID的充电站详情
            </summary>
            <param name="id">充电站ID</param>
            <remarks>
            请求示例:
            
                GET /api/ChargingStation/123e4567-e89b-12d3-a456-426614174000
            </remarks>
            <response code="200">返回充电站详情</response>
            <response code="404">指定ID的充电站不存在</response>
        </member>
        <member name="T:ChargingPile.API.Controllers.UserController">
